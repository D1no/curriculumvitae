{
  "semi": true,
  "trailingComma": "all",
  "singleQuote": false,
  "printWidth": 80,
  "importOrder": [
    "^react",
    "^react(.*)$",

    "^@(.*)$",

    "views",
    "^views/(.*)$",

    "containers",
    "^containers/(.*)$",

    "components",
    "^components/(.*)$",

    "assets",
    "^assets/(.*)$",

    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
